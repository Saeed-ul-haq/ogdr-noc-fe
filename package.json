  "private": true,
  "scripts": {
    "start": "run-s dev",
    "dev": "cross-env NODE_ENV=development node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.js",
    "test": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.js",
    "debug": "cross-env NODE_ENV=development webpack-dev-server --config webpack.debug.js",
    "build": "run-s lint build:no-lint",
    "analyze": "run-s lint clean build:analyze",
    "build:no-lint": "run-s clean build:webpack",
    "build:webpack": "cross-env NODE_ENV=production BABEL_ENV=post-production node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack --config webpack.prod.js",
    "build:analyze": "cross-env NODE_ENV=production webpack --config webpack.analyze.js",
    "clean": "rimraf dist",
    "lint": "run-p lint:js lint:style",
    "lint:js": "eslint \"**/*.js\"",
    "lint:style": "stylelint \"**/*.@(css|sass|scss)\"",
    "format": "run-p format:js format:style format:others",
    "format:js": "prettier-eslint --write \"**/*.js\"",
    "format:style": "run-s format:style:prettier format:style:stylelint",
    "format:others": "prettier --write \"**/*.@(json|gql|graphql)\"",
    "format:style:prettier": "prettier --write \"**/*.@(css|sass|scss)\"",
    "format:style:stylelint": "stylelint \"**/*.@(css|sass|scss)\" --fix",
    "precommit": "lint-staged",
    "prepush": "run-s lint"
  },
  "lint-staged": {
    "**/*.@(js|jsx)": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ],
    "**/*.@(css|sass|scss)": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "**/*.@(json|gql|graphql)": [
      "prettier --write",
      "git add"
    ]
  },
